<extension xmlns="http://ns.adobe.com/air/extension/2.5">
  <id>com.adobe.gyroscope</id>
  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libgyroscopeExtension.a</nativeLibrary>
                <initializer>ADBEExtInitializer</initializer>
                <finalizer>ADBEExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>GyroscopeNative.jar</nativeLibrary>
        <initializer>com.adobe.nativeExtension.GyroscopeExtension</initializer>
        <finalizer>com.adobe.nativeExtension.GyroscopeExtension</finalizer>
      </applicationDeployment>
    </platform>
  </platforms>
</extension>

